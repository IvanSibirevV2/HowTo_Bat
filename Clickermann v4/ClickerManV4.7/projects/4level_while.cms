// 4х уровневый цикл While

while($a < 3)
   
   $b = 0 // обнуление переменной цикла 2 уровня
   while($b < 3)
      
      $c = 0 // обнуление переменной цикла 3 уровня
      while($c < 3)
         
         $d = 0 // обнуление п. цикла 4 уровня
         while($d < 3)
            print($a, $b, $c, $d)
            inc($d)
         end_cyc
         inc($c)
         
      end_cyc
      inc($b)
      
   end_cyc
   inc($a)
   
end_cyc



halt // for single run


