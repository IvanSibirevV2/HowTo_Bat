#name "New Script"
$t_WAITMS=50
SUB(_WAITMS)
	WAITMS($t_WAITMS)
END_SUB
print("Hello, world!")
$n_Step=10
SUB(_MouseDanStep)
	FOR($a, $a < $n_Step)
		_WAITMS()
		MOVER(100,0)
		_WAITMS()
		MOVER(0,100)
		_WAITMS()
		MOVER(-100,0)
		_WAITMS()
		MOVER(0,-100)
		_WAITMS()
	END_CYC
END_SUB
_MouseDanStep()
HALT(1)